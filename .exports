export UNAME_MACHINE="$(/usr/bin/uname -m)"
if [[ "${UNAME_MACHINE}" == "arm64" ]]; then
    # On ARM macOS, this script installs to /opt/homebrew only
    export HOMEBREW_PREFIX="/opt/homebrew"
else
    # On Intel macOS, this script installs to /usr/local only
    export HOMEBREW_PREFIX="/usr/local"
fi
export NVM_DIR="$HOME/.nvm"
export EDITOR='vim'
export HISTSIZE='32768';
export HISTFILESIZE="${HISTSIZE}";
export HISTCONTROL='ignoreboth';
export __P_PROJECT_PATH="$HOME/Documents/workspace"
export __P_MAX_DEPTH=1
export __P_POST_CMD="(git config --get remote.origin.url && (git pull --all -q || git fetch --all -q)) &> /dev/null &"
export GOPATH="$HOME/.go"
export GOROOT="${HOMEBREW_PREFIX}/opt/go/libexec"
export PYENV_VIRTUALENV_DISABLE_PROMPT=1
export GPG_TTY=$(tty)
export _ZO_ECHO=1
export _ZO_EXCLUDE_DIRS="$HOME/.cache:$HOME/.local/share:$HOME/Library/Caches:\
.git:.hg:.svn:\
.idea:.vscode:.fleet:\
node_modules:\
dist:build:out:target:\
.gradle:.gradle:\
.next:.nuxt:.angular:\
coverage:\
__pycache__:\
.venv:venv:\
.mypy_cache:.pytest_cache:\
.DS_Store:\
.tmp:tmp"